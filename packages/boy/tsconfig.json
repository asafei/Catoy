{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext", 
      "jsx": "react", 
      "strict": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "inlineSourceMap": true,
      "noEmit": false,
      "skipLibCheck": true,
      "moduleResolution": "node",
      "types" : ["node"]
    },
    "include": [
      "src"
    ],
    "exclude": [
      "node_modules"
    ],
  }